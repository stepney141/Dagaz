(define drop-other (
   (verify (in-zone? home))
   (verify empty?)
   add
))

(define drop-flag (
   (verify (in-zone? hqf))
   (verify empty?)
   add
))

(define drop-bomb (
   (verify (in-zone? front))
   (verify empty?)
   add
))

(define drop-landmine (
   (verify (in-zone? rear))
   (verify empty?)
   add
))

(define common-step (
   $1 (verify not-friend?)
   add
))

(define common-slide (
  $1 
  (while empty? add $1)
  (verify not-friend?)
  add
))

